{
    "name": "mrdindar/blade-iranian-brands-icons",
    "description": "A package to easily make use of Iranian Brands Icons in your Laravel Blade views.",
    "keywords": ["Blade", "Iranian Brands", "Laravel"],
    "homepage": "https://github.com/rezadindar/blade-iranian-brands-icons",
    "license": "MIT",
    "authors": [
        {
            "name": "MRDindar"
        }
    ],
    "require": {
        "php": "^8.0",
        "blade-ui-kit/blade-icons": "^1.6",
        "illuminate/support": "^9.0|^10.0|^11.0|^12.0"
    },
    "require-dev": {
        "orchestra/testbench": "^10.0",
        "phpunit/phpunit": "^11.0"
    },
    "autoload": {
        "psr-4": {
            "MRDindar\\IranianBrandsIcons\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "MRDindar\\IranianBrandsIcons\\BladeIranianBrandsIconsServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
